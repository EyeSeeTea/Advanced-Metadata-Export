{
  "name": "advanced-import-export",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dhis2/d2-ui-app": "^1.0.5",
    "@dhis2/d2-ui-core": "^1.2.0",
    "@dhis2/d2-ui-header-bar": "^1.1.1",
    "@material-ui/core": "^1.2.3",
    "d2": "^30.0.2",
    "d2-manifest": "^1.0.0",
    "d2-utilizr": "^0.2.16",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "cd node_modules && cd d2-ui && npm install",
