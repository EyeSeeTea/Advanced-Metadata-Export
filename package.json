{
    "name": "advanced-import-export",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "d2": "^28.0.0",
        "d2-ui": "^28.0.0",
        "d2-manifest": "^1.0.0",
        "d2-utilizr": "^0.2.16",
        "file-saver": "^1.3.8",
        "jquery": "^3.3.1",
        "lodash": "^4.17.10",
        "material-ui": "0.20.0",
        "material-ui-icons": "^1.0.0-beta.36",
        "pouchdb": "^7.0.0",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-tap-event-plugin": "^2.0.1",
        "react-scripts": "1.1.4",
        "rxjs": "^5.2.0",
        "traverse": "^0.6.6",
        "prop-types": "latest"
    },
    "scripts": {
        "start": "react-scripts start",
        "prebuild": "rm -rf advanced-import-export.zip && rm -rf build/",
        "build": "react-scripts build",
        "postbuild": "d2-manifest package.json build/manifest.webapp && cd build && zip -r ../advanced-import-export.zip . && rm asset-manifest.json && rm service-worker.js && cd ..",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "manifest.webapp": {
        "name": "Advanced Import Export",
        "description": "Import/Export App with recursive metadata fetching",
        "version": "0.1",
        "developer": {
            "name": "Alexis Rico",
            "url": "https://github.com/SferaDev"
        },
        "icons": {
            "48": "img/import_export.svg"
        },
        "activities": {
            "dhis": {
                "href": "*"
            }
        }
    },
    "homepage": "."
}
